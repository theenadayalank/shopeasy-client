    "http-server": "^0.11.1",
    "husky": "^3.0.5",
    "lint-prepush": "^0.4.1",
    "mini-css-extract-plugin": "^0.8.0",
    "postcss-loader": "^3.0.0",
    "postcss-scss": "^2.0.0",
    "prettier": "^1.18.2",
